{
    "name": "uvdesk/community-skeleton",
    "description": "UVDesk Community Helpdesk Project Skeleton",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "doctrine/doctrine-migrations-bundle": "^2.2.2",
        "doctrine/orm": "^2.9",
        "google/recaptcha": "^1.2.4",
        "knplabs/knp-paginator-bundle": "^5.4.0",
        "korotovsky/sso-sp-bundle": "0.3.x-dev",
        "korotovsky/sso-library": "0.3.x-dev",
        "ocramius/proxy-manager": "2.2.3",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^5.5.3",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.12.2",
        "symfony/form": "^4.4.25",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mime": "4.4.*",
        "symfony/monolog-bundle": "^3.6.0",
        "symfony/process": "4.4.*",
        "symfony/property-access": "^4.4",
        "symfony/property-info": "^4.4",
        "symfony/proxy-manager-bridge": "^4.4",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "^4.4",
        "symfony/swiftmailer-bundle": "^3.4.0",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/var-dumper": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "uvdesk/api-bundle": "^1.0",
        "uvdesk/automation-bundle": "dev-master",
        "uvdesk/composer-plugin": "^1.0",
        "uvdesk/core-framework": "dev-master",
        "uvdesk/extension-framework": "^1.0",
        "uvdesk/mailbox-component": "^1.0",
        "uvdesk/support-center-bundle": "^1.0",
        "webmatic/kunde-bundle": "dev-portal-client",
        "webmatic/portal-client-bundle": "^1.0.0",
        "webmatic/user-bundle": "dev-portal-client",
        "webmatic/uvdesk-kunde-extension": "^1.0.2",
        "webmatic/uvdesk-sla-extension": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "4.4.*",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "uvdesk/composer-plugin": true
        },
        "classmap-authoritative": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "UVDesk\\CommunityPackages\\Webmatic\\UVDeskSLAExtension\\": "apps/webmatic/uvdesk-sla-extension/src/",
            "UVDesk\\CommunityPackages\\Webmatic\\UVDeskKundeExtension\\": "apps/webmatic/uvdesk-kunde-extension/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "for uvext in vendor/webmatic/uvdesk-*-extension;do ln -fsr \"$uvext\" apps/webmatic/;done": "script",
            "composer dumpautoload": "script",
            "rm -rf var/cache/*/": "script",
            "cache:warmup": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "uvdesk_extensions:configure-extensions": "symfony-cmd",
            "cp vendor/webmatic/*/Migrations/Version*.php migrations": "script",
            "cp apps/webmatic/*/src/Migrations/Version*.php migrations": "script",
            "cp -r apps/webmatic/*/src/Resources/views/bundles templates/": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*",
            "docker": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/bundles/doctrine-bundle.git"
        },
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/bundles/form-bundle.git"
        },
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/bundles/kunde-bundle.git"
        },
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/bundles/portal-client-bundle.git"
        },
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/bundles/user-bundle.git"
        },
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/bundles/twig-bundle.git"
        },
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/bundles/util-bundle.git"
        },
        {
            "type": "git",
            "url": "git@github.com:WebmaticMerseburg/automation-bundle.git"
        },
        {
            "type": "git",
            "url": "git@github.com:WebmaticMerseburg/core-framework.git"
        },
        {
            "type": "git",
            "url": "git@github.com:WebmaticMerseburg/SingleSignOnLibrary.git"
        },
        {
            "type": "git",
            "url": "git@github.com:WebmaticMerseburg/SingleSignOnServiceProviderBundle.git"
        },
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/uvdesk/kunde-extension.git"
        },
        {
            "type": "git",
            "url": "git@git.webmatic.de:webmatic/uvdesk/sla-extension.git"
        }
    ]
}
